<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" fx:id="parent" prefHeight="592.0" prefWidth="320" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="hotels.LoginViewController">
    <children>
        <Button fx:id="loginButton" layoutX="110.0" layoutY="139.0" onAction="#loginHandler" text="Login" />
      <TextField fx:id="usernameField" layoutX="86.0" layoutY="52.0" />
      <Label layoutX="23.0" layoutY="56.0" text="Username:" />
      <Label layoutX="25.0" layoutY="92.0" text="Password:" />
      <Label layoutX="133.0" layoutY="14.0" text="Login">
         <font>
            <Font size="16.0" />
         </font>
      </Label>
      <Label fx:id="errorLabel" layoutX="86.0" layoutY="119.0" text="Invalid username or password." textFill="RED" visible="false">
         <font>
            <Font size="10.0" />
         </font>
      </Label>
      <PasswordField fx:id="passwordField" layoutX="86.0" layoutY="88.0" />
      <Button fx:id="newUserButton" layoutX="173.0" layoutY="139.0" mnemonicParsing="false" onAction="#newUserHandler" text="New User" />
    </children>
</AnchorPane>
