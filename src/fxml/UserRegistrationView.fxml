<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" fx:id="parent" prefHeight="400.0" prefWidth="369.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="hotels.UserRegistrationViewController">
   <children>
      <Label layoutX="14.0" layoutY="14.0" text="New User Registration">
         <font>
            <Font size="16.0" />
         </font>
      </Label>
      <TextField fx:id="usernameField" layoutX="156.0" layoutY="102.0" />
      <Label layoutX="83.0" layoutY="106.0" text="Username:" />
      <PasswordField fx:id="passwordField" layoutX="156.0" layoutY="140.0" />
      <Label layoutX="85.0" layoutY="144.0" text="Password:" />
      <PasswordField fx:id="confirmPasswordField" layoutX="156.0" layoutY="176.0" />
      <Label layoutX="40.0" layoutY="180.0" text="Confirm Password:" />
      <TextField fx:id="emailField" layoutX="156.0" layoutY="215.0" />
      <Label layoutX="107.0" layoutY="219.0" text="Email:" />
      <Button fx:id="submitButton" layoutX="251.0" layoutY="293.0" mnemonicParsing="false" onAction="#submitHandler" text="Submit" />
      <Button fx:id="cancelButton" layoutX="156.0" layoutY="293.0" mnemonicParsing="false" onAction="#cancelHandler" text="Cancel" />
